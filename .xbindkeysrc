# vi ft=shell

"firefox"
    Control + Alt + f
"chromium"
    Control + Alt + c
"skype"
    Control + Alt + s
"pidgin"
    Control + Alt + p
"dropboxd"
    Control + Alt + d
"~/.scripts/switch_todo_stock"
    Control + Alt + w
"~/.scripts/start_proxy"
    Control + Alt + Shift + p
"~/.scripts/todo m"
    Control + Alt + t
"~/.scripts/todo r"
    Control + Alt + Shift + t
"javaws ~/Dropbox/stock/jforex.jnlp"
    Control + Alt + j

# Sound
"amixer sset Master 5%- unmute"
    XF86AudioLowerVolume
"amixer sset Master 5%+ unmute"
    XF86AudioRaiseVolume
"amixer sset Master toggle"
    XF86AudioMute
