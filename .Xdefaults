!URxvt
URxvt*background: #000000
URxvt*foreground: #DEDEDE
URxvt*saveLines:  200000
URxvt.depth:       24
URxvt.buffered:    true
URxvt.transparent: false
URxvt.shading:     100

!Scroll
URxvt.scrollBar: true
URxvt.scrollBar_right: true
URxvt.scrollBar_floating: true
URxvt.scrollstyle: plain
URxvt.saveLines: 50000

urxvt*buffered:false
urxvt*transparent:true
urxvt*fading:20
urxvt*tintColor:000000
urxvt*shading:30
urxvt*preeditType:Root

urxvt*inputMethod:fcitx
urxvt*imLocale:zh_CN.utf8
URxvt.font:xft:Bitstream Vera Sans Mono,xft:WenQuanYi Bitmap Song
URxvt.boldFont:xft:Bitstream Vera Sans Mono:Bold,xft:WenQuanYi Bitmap Song:Bold
urxvt*urgentOnBell:true
urxvt*meta8:false
urxvt*borderless:true
urxvt*multichar_encoding:utf8
urxvt*cursorBlink:false
urxvt*cursorUnderline:false
urxvt*metaSendsEscape: true
urxvt*eightBitInput: false

# for url yanking
URxvt.perl-ext-common:  default,selection,url-select,clipboard
URxvt.keysym.M-u:       perl:url-select:select_next
URxvt.urlLauncher:      /usr/bin/chromium
URxvt.underlineURLs:    true
URxvt*underlineURLs: true
URxvt.keysym.M-p:      perl:clipboard:paste
URxvt*matcher.button:   1


Xft*dpi: 96
Xft*hinting: true
Xft*hintstyle: hintmedium
Xft*antialias: true
Xft*rgba: rgb
