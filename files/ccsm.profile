[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[expo]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[unityshell]
s0_launcher_hide_mode = 1
s0_num_launchers = 1
s0_reveal_pressure = 5
s0_backlight_mode = 2
s0_disable_show_desktop = true

[core]
s0_active_plugins = core;composite;opengl;copytex;decor;compiztoolbox;imgpng;resize;regex;put;grid;mousepoll;widget;snap;vpswitch;move;animation;gnomecompat;wall;place;annotate;unitymtgrabhandles;expo;workarounds;session;ezoom;fade;scale;unityshell;
s0_outputs = 1600x900+0+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_hsize = 2
s0_vsize = 2

[put]
s0_put_next_output_key = <Shift><Super>e

[ezoom]
s0_zoom_in_key = <Super>equal
s0_zoom_out_key = <Super>minus

[thumbnail]
s0_thumb_color = #0000007f
s0_font_background_color = #0000007f

[animation]
s0_unminimize_effects = animation:Glide 2;

[showmouse]
s0_color = #ffdf3fff

[cubeaddon]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[firepaint]
s0_fire_color = #ff3305ff

[shift]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[resizeinfo]
s0_gradient_1 = #cccce6cc
s0_gradient_2 = #f3f3f3cc
s0_gradient_3 = #d9d9d9cc
s0_outline_color = #e6e6e6ff

[gnomecompat]
s0_main_menu_key = Disabled
s0_run_key = Disabled
s0_command_window_screenshot = gnome-screenshot -w
s0_run_command_terminal_key = <Control><Alt>t

[wall]
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9

[place]
s0_mode_matches = class=Gnome-terminal;
s0_mode_modes = 3;
s0_viewport_matches = class=Empathy;
s0_viewport_x_values = 2;
s0_viewport_y_values = 2;

[annotate]
s0_initiate_line_button = <Control><Alt>Button2
s0_initiate_rectangle_button = <Control><Alt><Super>Button1
s0_initiate_ellipse_button = <Control><Alt>Button1
s0_erase_button = <Shift><Control><Alt><Super>Button1

